package bytebank;

public class Conta {
	double saldo;
	int agencia;
	int numero;
	String titular;

	// função depositar
	public void depositar(double valor) {

		this.saldo += valor;

	}


 public boolean sacar(double valor) {
	if(this.saldo <= valor) {
		this.saldo -= valor;
		return true;
	}
	
 }
	
	
}
